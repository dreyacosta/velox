vendor(prop, args)
  -webkit-{prop} args
  -moz-{prop} args
  -ms-{prop} args
  -o-{prop} args
  {prop} args

vendorVal(prop, args)
  {prop} -webkit- + args
  {prop} -moz- + args
  {prop} -ms- + args
  {prop} -o- + args
  {prop} args

